{
	"name" : "webservco/hello-world",
	"type" : "project",
	"description" : "Sample App for the WebServCo PHP Framework",
	"keywords" : [
		"framework",
		"webservco",
		"demo"
	],
	"homepage" : "https://webserv.co",
	"license" : "MIT",
	"authors" : [{
			"name" : "Radu Maier",
			"email" : "ariademur@webserv.co"
		}
	],
	"repositories" : [{
			"type" : "path",
			"url" : "~/projects/local/webservco/framework",
			"options" : {
				"symlink" : true
			}
		}
	],
	"require" : {
		"php" : "^5.4 || ^7.0"
	},
	"require-dev" : {
		"pds/skeleton" : "~1.0",
		"squizlabs/php_codesniffer" : "3.*",
		"webservco/framework" : "@dev"
	},
	"autoload" : {
		"psr-4" : {
			"Project\\" : "src/"
		}
	},
	"minimum-stability" : "stable",
	"scripts" : {
		"check:structure" : "vendor/bin/pds-skeleton validate",
		"check" : "vendor/bin/phpcs --standard=PSR2 --extensions=php src public"
	}
}
